{
  "name": "pytania",
  "version": "1.0.0",
  "description": "A fun Quiz App",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --apply src",
    "type-check": "tsc --noEmit --incremental false",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "bun run src/lib/db/migrate.ts",
    "db:seed": "bun run src/lib/db/seed.ts",
    "db:drop": "drizzle-kit drop",
    "db:pull": "drizzle-kit introspect:pg",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check:pg",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose stop",
    "db:rm": "docker compose down -v",
    "email": "email dev --dir ./src/lib/email/templates --port 3001",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.15",
    "@hookform/resolvers": "^3.3.4",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@neondatabase/serverless": "^0.7.2",
    "@next/mdx": "^14.1.0",
    "@nobara/next": "^0.1.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@react-email/components": "^0.0.14",
    "@upstash/ratelimit": "^1.0.0",
    "@upstash/redis": "^1.28.1",
    "drizzle-orm": "^0.29.3",
    "million": "^2.6.4",
    "next": "14.1.0",
    "next-auth": "beta",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "^2.0.0",
    "react-hook-form": "^7.49.3",
    "redaxios": "^0.5.1",
    "resend": "^3.0.0",
    "sonner": "^1.3.1",
    "valibot": "^0.26.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/cz-commitlint": "^18.4.4",
    "@radix-ui/colors": "^3.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "drizzle-kit": "^0.20.13",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "mizuhara": "^2.4.0",
    "pg": "^8.11.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "windy-radix-palette": "^2.0.0-beta.7"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": ["biome check --apply"],
    "*.{md,mdx,json}": ["biome format --write"]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
